; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SkyknightsOnline"
run/main_scene="res://src/Main.tscn"
config/features=PackedStringArray("4.0")
boot_splash/image="res://assets/splash.png"
boot_splash/fullsize=false
config/icon="res://assets/icon/Skyknights.svg"

[autoload]

Stopwatch="*res://src/system/Stopwatch.gd"
Utils="*res://src/system/Utils.gd"
Files="*res://src/system/Files.gd"
Args="*res://src/system/Args.gd"
Settings="*res://src/ui/settings/Settings.tscn"
InputManager="*res://src/system/InputManager.gd"
Game="*res://src/system/Game.gd"
Player="*res://src/player/Player.tscn"
MainMenu="*res://src/ui/MainMenu.tscn"
HUD="*res://src/ui/hud/HUD.tscn"
Scenes="*res://src/system/Scenes.gd"
ShipManager="*res://src/system/ShipManager.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/return_value_discarded=false
gdscript/warnings/unused_argument=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/zylann.editor_light/plugin.cfg")

[input]

ui_accept={
