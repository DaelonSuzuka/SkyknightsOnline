; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="SkyknightsOnline"
run/main_scene="res://src/Main.tscn"
config/icon="res://icon.png"

[autoload]

InputManager="*res://src/InputManager.gd"
MainMenu="*res://src/ui/MainMenu.tscn"
HUD="*res://src/ui/hud/HUD.tscn"
Console="*res://addons/quentincaffeino/console/src/Console.tscn"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unused_variable=false
multirun/number_of_windows=3
multirun/add_custom_args=true
multirun/first_window_args="--server=1 "
multirun/other_window_args="--connect=1"
multirun/window_distance=200

[editor_plugins]

enabled=PoolStringArray( "res://addons/RadialMenu/plugin.cfg", "res://addons/multirun/plugin.cfg", "res://addons/quentincaffeino/console/plugin.cfg", "res://addons/ui_design_tool/plugin.cfg" )

